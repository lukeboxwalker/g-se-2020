<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="de.techfak.se.lwalkenhorst.conrtoller.LobbyController"
            prefHeight="400.0" prefWidth="600.0">
    <HBox style="-fx-alignment: CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
          AnchorPane.rightAnchor="0.0"
          AnchorPane.topAnchor="0.0">
        <Button fx:id="startButton"
                style="-fx-base: white; -fx-focus-color: transparent;" prefHeight="35" text="Start">
            <font>
                <Font name="arial" size="15"/>
            </font>
        </Button>
    </HBox>
</AnchorPane>
